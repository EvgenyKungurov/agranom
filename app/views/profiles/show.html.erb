<%= render 'layouts/dashboard_nav' %>

<div class="profile-info">
<p id="notice"><%= notice %></p>
<p><%= t('profile_info') %></p>
  <div class="panel panel-info"> 
    <div class="panel-heading">
      <h3 class="panel-title"><%= t('your_name')  %></h3>
    </div>
    <div class="panel-body"><%= @profile.name %></div> 
  </div>
  <div class="panel panel-info"> 
    <div class="panel-heading">
      <h3 class="panel-title"><%= t('city') %></h3>
    </div>
    <div class="panel-body"><%= @profile.user.city.name %></div> 
  </div>
<%= link_to t('edit'), edit_profile_path(@profile), class: 'btn btn-success' %> 
</div>