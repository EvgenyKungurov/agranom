<div class="search">
  <div class="center-block col-md-9 col-sm-9 col-lg-9">
    <%= render 'layouts/search' %>
    <br>
    <%= link_to "Подать объявление", new_profile_ad_path(current_user.profile) , class: "btn btn-success", 'data-turbolinks': false %>
  </div>
</div>
<br>

<% @result.each do |ad| %>
<div class="row">
  <div class="bs-callout">
    <div class="row">
      <div class="col-md-2">
        <img alt="140x140" data-src="holder.js/140x140" class="img-rounded" style="width: 140px; height: 140px;" src="data:image/svg+xml;base64," data-holder-rendered="true">
      </div>
      <div class="col-md-8">
        <h4><%= link_to ad.title, ad_path(ad.location.slug, ad.category.slug, ad.slug) %></h4>
        <h4><%= "#{t('expire_day')}: #{l(ad.expire_day)}" %></h4>
        <h4><%= "#{t('price')}: #{ad.price}" %></h4> 
        <h4>
      </div>   
    </div>
  </div>
</div>
<% end %>